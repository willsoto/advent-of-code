{
  "name": "@willsoto/aoc-2021",
  "version": "0.1.0",
  "private": true,
  "description": "",
  "keywords": [],
  "license": "Apache-2.0",
  "author": "Will Soto <willsoto@users.noreply.github.com> (https://github.com/willsoto)",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "test": "ava",
    "test:watch": "ava --watch"
  },
  "prettier": {
    "arrowParens": "always",
    "semi": true,
    "singleQuote": false,
    "trailingComma": "all"
  },
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "files": [
      "src/**/*.spec.ts"
    ],
    "nodeArguments": [
      "--loader=ts-node/esm",
      "--no-warnings"
    ],
    "nonSemVerExperiments": {
      "configurableModuleFormat": true
    },
    "verbose": true
  },
  "dependencies": {
    "lodash": "~4.17.21"
  },
  "devDependencies": {
    "@types/lodash": "~4.14.178",
    "@types/node": "~16.11.12",
    "ava": "~3.15.0",
    "prettier": "~2.5.1",
    "prettier-plugin-organize-imports": "~2.3.4",
    "prettier-plugin-packagejson": "~2.2.15",
    "ts-node": "~10.4.0",
    "typescript": "~4.5.3"
  },
  "volta": {
    "node": "16.13.1",
    "npm": "8.3.0"
  }
}
